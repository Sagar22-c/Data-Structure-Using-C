#include<stdio.h>
#include<conio.h>
#include<string.h>
void main()
{
 char arr[5][20] = {"apple", "banana", "cherry", "grape", "orange"};
 char search[20];
 int i, found=0;
 clrscr();

 printf("Enter string to search:");
 scanf("%s",search);

 for(i=0;i<5;i++)
 {
  if(strcmp(arr[i],search) == 0)
  {
   printf("String found at index %d.\n",i);
   found=1;
   break;
  }
 }
 if(!found)
 {
  printf("String not found in the array.\n");
 }
 getch();
}
